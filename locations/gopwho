# gopwho
VasanTut = 1
brand = rand(1, 100)

if brand >= 40:
	BerezaTut = 1
else
	BerezaTut = 0
end

vrand = rand(1, 100)

if vrand >= 50:
	VitekTut = 1
else
	VitekTut = 0
end

krand = rand(1, 100)

if krand >= 60:
	KostilTut = 1
else
	KostilTut = 0
end

urand = rand(1, 100)

if urand >= 70:
	UdmurtTut = 1
else
	UdmurtTut = 0
end

srand = rand(1, 100)

if srand >= 80:
	SeriTut = 1
else
	SeriTut = 0
end

