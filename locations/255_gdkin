$din_reakt_dance = {
	if alko > 0:
		horny += 10
		manna += 5
		'Do you feel that the actions of Man excite you.'
	else
		if horny > 0:horny -= 5

		manna -= 5
		'You feel disgusted by these actions Man.'
	end

	gs 'stat'
}

