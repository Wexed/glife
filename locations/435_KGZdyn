$KGZvihod10 = {
if KGZplaceA < 10:$KGZplaceA = 'Fort Bragg' & $KGZplaceB = 'from the city'
if KGZplaceA > 10:$KGZplaceA = 'from the city' & $KGZplaceB = 'Fort Bragg'
