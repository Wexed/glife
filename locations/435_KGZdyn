if KGZweapon = 2:$KGZweapon = 'Ax' & $KGZweapona = 'Ax'
if KGZgun > 0:$KGZgun = 'Pistol, cartridges <<bullet>>piece'
if KGZgun2 > 0:$KGZgun2 = 'Shotgun cartridges <<bullet2>>piece'
if KGZgun3 > 0:$KGZgun3 = 'Automatic, cartridges <<bullet3>>piece'

if KGZhour >= 24:
KGZhour -= 24
KGZday += 1
	if KGZsup = 0:
	KGZHP -= 10
	else
	KGZsup -= 1
	end

