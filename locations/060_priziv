if $ARGS[0] = 'sasori':
		set $nameV = 'Sasori doll'
		!Статы
		set strenV = 100
	if SasoriBody = 5:
		set speedV = 100
		set agilV = 100
		set reactV = 100
	end
	if SasoriBody = 4:
		set speedV = 110
		set agilV = 100
		set reactV = 100
	end
	if SasoriBody = 3:
		set speedV = 120
		set agilV = 105
		set reactV = 105
	end
	if SasoriBody = 2:
		set speedV = 130
		set agilV = 110
		set reactV = 110
	end
	set vitalV = 100
	set intelV = 100
	set willV = 100
	!Навыки
	set magikV = 10
	set boxingV = 100
	set shootV = 100
