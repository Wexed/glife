if $ARGS[0] = 'tobi':
	set $nameV      = 'Toby'
	!Статы
	set strenV      = 300
	set speedV      = 100
	set agilV       = 100
	set reactV      = 100
	set vitalV      = 100
	set intelV      = 100
	set willV       = 200
	!Навыки
	set magikV      = 12
	set boxingV     = 100
	set shootV      = 100
	!Параметры
	set healthV     = vitalV * 10 + strenV * 5
	set mannaV      = intelV * magikV + magikV * 100 + vitalV * 10
	set willpowerV  = willV * 10
	set BonusAtakV  = 0
	set BonusDefV   = 0
	!программа действий
	set program     = 6
	set unmaterialV = 40

	set stunV       = 0
=======
