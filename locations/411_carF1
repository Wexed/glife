# carF1
if $ARGS[0]='car':
$metka = $ARGS[0]
$loc = $CURLOC
cla
*clr
gs'stat'
set minut = minut + 5
clr
'<center><B>VAZ 2107</B></center>'
'<center><img src="picar/car.jpg" ></center>'

     act 'Sit in the salon':
     cla
     *clr
     '<center><img src="picar/salon.jpg" ></center>'
     act 'Get_Away':gt'carF1','car'
     end

     act 'Get_Away':
     cla
     if cartraid = 1:gt'autosalonF','start'
     if cartraid = 2:gt'autotraidF','manager'
     end
end


