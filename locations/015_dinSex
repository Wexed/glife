$veneradinitog = {
	if tipvenerarand >= 8:
		!!herpes
		set gerprand = rand(0, 100)

		if gerprand > 90:
			if GerpesOnce = 0:set GerpesOnce = 1

			Venera += 1
			Gerpes += 1
		end
	end

	if tipvenerarand = 7:
		!!syphilis
		venerarand = rand(0, 100)

		if venerarand > 95:
			Venera += 1

			if SifacOnce = 0:SifacOnce = 1

			Sifilis += 1
		end
	end

	if tipvenerarand >= 5 and tipvenerarand < 7:
		!!gonorrhea/triper

		venerarand = rand(0, 100)

		if venerarand > 95:
			Venera += 1

			if TriperOnce = 0:TriperOnce = 1

			Triper += 1
		end
	end

	if tipvenerarand < 5:
		!!candidiasis/thrush

		venerarand = rand(0, 100)

		if venerarand > 85:
			if KandidozOnce = 0:KandidozOnce = 1

			Kandidoz += 1
		end
	end
}

