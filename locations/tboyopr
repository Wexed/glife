# tboyopr
if boynumBlock = 0:Tboynum = boynum

if boynumBlock > 0:
	if bmHJ[Tboynum] > 0 or bmBJ[Tboynum] > 0 or bmSEX[Tboynum] > 0 or bmANAL[Tboynum] > 0:
		if bmTOTSEX[Tboynum] = 0:bmTOTSEX[Tboynum] = 1
	end

	$bname = $bmNane[Tboynum]
	dick = bmDick[Tboynum]
	silavag = bmSila[Tboynum]
end

