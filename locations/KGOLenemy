# KGOLenemy
if $ARGS[0] = '0':
	$KGname = 'Predatory flower'
	KGOLstrenV = 10
	KGOLspeedV = 10
	KGOLvitalV = 10
	KGOLintelV = 10
	KGHPV = KGOLvitalV * 10
	KGMannaV = KGOLintelV * 10
	KGOLPrizeExp = rand(1, 3)
	KGOLPrizeM = rand(10, 30)
	KGOLennum = 0
	KGOLbronaV = 0
	KGOLneedDist = 10
	KGOLpowerV = KGOLintelV
end

